{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./src/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://doctors-one-flax.vercel.app/"
      }
    }
  ]
}
